{
	"name": "vinui-vscode-extension",
	"private": true,
	"displayName": "vinui-vscode-extension",
	"description": "vinui extension for vscode",
	"version": "0.1.0",
	"icon": "src/vingogo.png",
	"engines": {
		"vscode": "^1.66.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vingogo/vinui-vscode-extension.git"
	},
	"categories": [
		"Other"
	],
	"publisher": "vinui",
	"activationEvents": [
		"onLanguage:vue",
		"onLanguage:javascript",
		"onLanguage:typescript",
		"onLanguage:javascriptreact",
		"onLanguage:typescriptreact"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": []
	},
	"scripts": {
		"build": "webpack --mode production --devtool hidden-source-map",
		"package": "yarn build && vsce package",
		"publish": "vsce publish",
		"unpublish": "vsce unpublish vinui.vinui-vscode-extension"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/vscode": "^1.66.0",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"@vscode/test-electron": "^2.0.3",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"ts-loader": "^9.4.2",
		"webpack": "^5.84.0",
		"webpack-cli": "^5.1.1",
		"@vscode/vsce": "^2.7.0"
	}
}